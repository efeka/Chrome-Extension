{
	"manifest_version": 3,
	"name": "Tefas Extension",
	"version": "1.0",
	"content_scripts": [
		{
			"matches": [
				"<all_urls>"
			],
			"js": ["content.js"]
		}
	],
	"background": {
		"service_worker": "background.js"
	},
	"action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html",
		"default_title" : "Tefas Extension"
	},
	"permissions": [
	    "storage"
	]
}